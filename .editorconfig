# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.py]
indent_size = 4
indent_style = space
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[.travis.yml]
indent_size = 2
indent_style = space

[*.html]
indent_size = 2
indent_style = space

[*.css]
indent_size = 2
indent_style = space
